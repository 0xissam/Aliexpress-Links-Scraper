{
    "manifest_version": 3,
    "name": "Aliexpress Links Scraper",
    "version": "2.0",
    "description": "extracts all products links fron aliexpress website",
    "permissions": ["scripting", "activeTab", "clipboardWrite","storage", "declarativeContent"],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
    
  }
  