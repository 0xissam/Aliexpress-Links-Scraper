{
    "manifest_version": 3,
    "name": "Products Links Scraper",
    "version": "1.0",
    "description": "extracts all products links fron aliexpress website",
    "permissions": ["scripting", "activeTab", "clipboardWrite","storage", "declarativeContent"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      },
        "icons": {
          "16": "images/icon16.png",
          "48": "images/icon48.png",
          "128": "images/icon128.png"
        }
    }
    
  }
  